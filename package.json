{
  "name": "yumetodo",
  "version": "1.0.4",
  "description": "Hatena Blog Theme (yumetodo customized)",
  "main": "boilerplate/header_adjuster.js",
  "scripts": {
    "install": "node closure-compiler-install.js",
    "build": "npm-run-all --parallel build:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint boilerplate/header_adjuster.js closure-compiler-install.js",
    "lesshint": "lesshint boilerplate/",
    "lint": "npm-run-all --parallel eslint lesshint",
    "build:less": "lessc --clean-css=\"--s1 --advanced\" boilerplate/boilerplate.less bin/style.min.css",
    "build:js": "java -jar closure-compiler.jar --compilation_level=ADVANCED  --js='boilerplate/header_adjuster.js' --js_output_file='bin/header_adjuster.min.js' --language_out=ECMASCRIPT5_STRICT -W=VERBOSE --charset='UTF-8'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumetodo/Hatena-Blog-Themes.git"
  },
  "keywords": [
    "themes",
    "Hatena",
    "Blog",
    "Responsive"
  ],
  "author": "yumetodo <yume-wikijp@live.jp> (https://github.com/yumetodo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yumetodo/Hatena-Blog-Themes/issues"
  },
  "homepage": "https://github.com/yumetodo/Hatena-Blog-Themes#readme",
  "devDependencies": {
    "download": "^5.0.3",
    "eslint": "^3.14.1",
    "eslint-plugin-node": "^3.0.5",
    "fs-promise": "^1.0.0",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "lesshint": "^2.4.0",
    "npm-run-all": "^4.0.1",
    "path": "^0.12.7"
  },
  "config": {
    "unsafe-perm":true
  }
}
